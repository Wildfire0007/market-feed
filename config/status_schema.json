{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["ok", "assets"],
  "properties": {
    "ok": {"type": "boolean"},
    "status": {"type": "string"},
    "generated_utc": {"type": "string"},
    "assets": {
      "type": "object",
      "minProperties": 1
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {"type": "string"},
          "message": {"type": "string"},
          "reset_utc": {"type": "string"}
        }
      }
    }
  },
  "additionalProperties": true
}
